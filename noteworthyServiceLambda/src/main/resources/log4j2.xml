<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="DEBUG">
    <Appenders>
        <Console name="ConsoleLogger" target="SYSTEM_OUT">
            <PatternLayout pattern=">>> %d{DATE} [%-5p] %X{AWSRequestId} (%t) %C:%L: %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="DEBUG">
            <AppenderRef ref="ConsoleLogger" />
        </Root>
    </Loggers>
</Configuration>